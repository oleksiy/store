<h1>Products</h1>

<%= link_to "Login", new_session_path unless authenticated? %>
<%= link_to "New Product", product_new_path if authenticated? %>

<div id="products">
  <% @products.each do |product| %>
    <div>
      <%= link_to product.name, product_path(product.id) %> 
    </div>
  <% end %>
</div>

